ddAdventure(this.state);\r\n        this.setState({\r\n            name: \"\",\r\n            image_url: \"\"\r\n        })\r\n    }\r\n\r\n    render() {\r\n        return(\r\n            <Form className=\"new-adventure-form\" onSubmit={this.handleSubmit}>\r\n            <h2>Exodus : Sierra Leone</h2>\r\n            \r\n            <label>Topic's Name </label>\r\n            <input\r\n                id=\"name\"\r\n                required value={this.state.name}\r\n                onChange={(e) => this.handleChange(e)}\r\n                />\r\n            \r\n\r\n            \r\n            <label>Image</label>\r\n            <textarea\r\n                id=\"image_url\"\r\n                required value={this.state.image_url}\r\n                onChange={this.handleChange} />\r\n            \r\n\r\n            <Button type=\"submit\">Add Entry</Button>\r\n        </Form>\r\n        )\r\n    }\r\n}\r\n\r\n\r\n\r\nexport default connect(null, { addAdventure })(AdventureInput);","/mnt/c/Users/angel/dev/flatiron/projects/Exodus/exodus_frontend/src/actions/AdventuresActions.js",["78"],"//Fetching adventures\r\n\r\nexport const fetchAdventures = () => {\r\n    return dispatch => {\r\n        dispatch({type: 'LOADING_ADVENTURES'})\r\n       return fetch('api/v1/adventures')\r\n        .then(res => res.json())\r\n        .then(adventures => dispatch({type: 'ADVENTURES_LOADED', payload: adventures})) \r\n    }\r\n}\r\n\r\n\r\n// fetching  adventures's id\r\n\r\nexport const fetchAdventure = id => {\r\n    return (dispatch) => {\r\n        dispatch({type: 'LOADING_ADVENTURES'})\r\n        return fetch('/api/v1/adventures/${id}')\r\n        .then(res => res.json())\r\n        .then(adventure => dispatch({type: 'ADVENTURE_LOADED', payload: adventure}))\r\n    }\r\n}\r\n\r\n\r\n// adding adventure\r\n\r\nexport const addAdventure = adventureForm => {\r\n    let data = {\r\n        method: 'POST',\r\n        Headers: {\r\n            'Accept': 'application/json',\r\n          